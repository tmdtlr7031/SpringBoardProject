<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<setting name="jdbcTypeForNull" value="NULL" /> <!-- 매핑되는 파라미터가 Null인경우 컬럼에 null값이 들어가게 설정-->
	</settings>
    <typeAliases>
		<typeAlias	alias="DefaultVO" type="nss.com.service.DefaultVO"/>
		<typeAlias	alias="BoardFileVO" type="nss.com.service.BoardFileVO"/>
		<typeAlias	alias="DashBoardVO" type="nss.dashboard.service.DashBoardVO"/>
		<typeAlias	alias="ComBoardVO" type="nss.fileboard.service.ComBoardVO"/>
		<typeAlias	alias="CommentVO" type="nss.fileboard.service.CommentVO"/>
    </typeAliases>
    
</configuration>
